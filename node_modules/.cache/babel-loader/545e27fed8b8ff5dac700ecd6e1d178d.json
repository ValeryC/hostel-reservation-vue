A,GAAA,CAAA,IAAA,EAAA;AACA,QAAA,CAAA,CAAA,EAAA,GAAA,GAAA,CAAA,EAAA;;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AACA,OAJA;AAKA,KAPA;AASA,IAAA,EAAA,CAAA,UAAA,CAAA,QAAA,EAAA,UAAA,CAAA,UAAA,aAAA,EAAA;AACA,MAAA,KAAA,CAAA,MAAA,GAAA,EAAA;AACA,MAAA,aAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,CAAA,GAAA,GAAA,CAAA,IAAA,EAAA;AACA,QAAA,CAAA,CAAA,EAAA,GAAA,GAAA,CAAA,EAAA;;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AACA,OAJA;AAKA,KAPA;AASA,IAAA,EAAA,CAAA,UAAA,CAAA,SAAA,EAAA,UAAA,CAAA,UAAA,aAAA,EAAA;AACA,MAAA,KAAA,CAAA,OAAA,GAAA,EAAA;AACA,MAAA,aAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,CAAA,GAAA,GAAA,CAAA,IAAA,EAAA;AACA,QAAA,CAAA,CAAA,EAAA,GAAA,GAAA,CAAA,EAAA;;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,CAAA;AACA,OAJA;AAKA,KAPA;AASA,IAAA,EAAA,CAAA,UAAA,CAAA,UAAA,EAAA,UAAA,CAAA,UAAA,aAAA,EAAA;AACA,MAAA,KAAA,CAAA,QAAA,GAAA,EAAA;AACA,MAAA,aAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,CAAA,GAAA,GAAA,CAAA,IAAA,EAAA;AACA,QAAA,CAAA,CAAA,EAAA,GAAA,GAAA,CAAA,EAAA;;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,CAAA;AACA,OAJA;AAKA,KAPA;AAQA,GA7FA;AA+FA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,qBACA,UADA,EACA,EADA,EACA;AACA,MAAA,EAAA,CAAA,UAAA,CAAA,UAAA,EACA,GADA,CACA,EADA,EAEA,MAFA,GAGA,IAHA,CAGA,YAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,+BAAA;AACA,OALA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,2BAAA,EAAA,KAAA;AACA,OARA;AASA;AAXA;AA/FA,CAAA","sourcesContent":["<template>\n  <div>\n    <b-navbar toggleable=\"lg\" type=\"dark\" variant=\"dark\">\n      <b-navbar-brand href=\"#\">\n        <i class=\"fas fa-hotel\"></i>\n        <!-- <img :src=\"require('../assets/nestor.jpg')\" height=\"20px\" width=\"20px\"> -->\n        Nestor reservation App\n      </b-navbar-brand>\n\n      <b-navbar-toggle target=\"nav-collapse\"></b-navbar-toggle>\n      <b-collapse id=\"nav-collapse\" is-nav>\n        <!-- Right aligned nav items -->\n        <b-navbar-nav class=\"ml-auto\">\n          <b-nav-form>\n            <router-link to=\"/\">\n              <b-button size=\"sm\" class=\"my-2 mr-2 my-sm-0\">\n                <i class=\"fas fa-door-open\"></i> Exit\n              </b-button>\n            </router-link>\n\n            <!-- Right aligned nav items -->\n            <router-link to=\"/admin\">\n              <b-button size=\"sm\" class=\"my-2 mr-2 my-sm-0\">\n                <i class=\"fas fa-user\"></i> Admin\n              </b-button>\n            </router-link>\n\n            <router-link to=\"/message\">\n              <b-button size=\"sm\" class=\"my-2 mr-2 my-sm-0\">\n                <i class=\"fas fa-envelope\"></i> Message\n              </b-button>\n            </router-link>\n          </b-nav-form>\n        </b-navbar-nav>\n      </b-collapse>\n    </b-navbar>\n    <b-container fluid class=\"bv-example-row mt-4\">\n      <b-row>\n        <b-col cols=\"12\">\n          <h2 class>Room-single</h2>\n          <b-img :src=\"require('@/assets/img1.jpg')\" fluid alt=\"Responsive image\"></b-img>\n          <b-table :fields=\"fields\" bordered striped hover :items=\"single\">\n            <template v-slot:cell(delete)=\"row\">\n              <b-button\n                size=\"md\"\n                variant=\"danger\"\n                block\n                @click=\"deletekey('Single',row.item.id)\"\n              >Delete Reservation</b-button>\n            </template>\n          </b-table>\n        </b-col>\n        <b-col cols=\"12\">\n          <h2 class>Room-Double</h2>\n          <b-img :src=\"require('@/assets/img2.jpg')\" fluid alt=\"Responsive image\"></b-img>\n          <b-table :fields=\"fields\" bordered striped hover :items=\"double\">\n            <template v-slot:cell(delete)=\"row\">\n              <b-button\n                size=\"md\"\n                variant=\"danger\"\n                block\n                @click=\"deletekey('Double',row.item.id)\"\n              >Delete Reservation</b-button>\n            </template>\n          </b-table>\n        </b-col>\n\n        <b-col cols=\"12\">\n          <h2 class>Room - Family</h2>\n          <b-img :src=\"require('@/assets/img3.jpg')\" fluid alt=\"Responsive image\"></b-img>\n          <b-table :fields=\"fields\" bordered striped hover :items=\"family\">\n            <template v-slot:cell(delete)=\"row\">\n              <b-button\n                size=\"md\"\n                variant=\"danger\"\n                block\n                @click=\"deletekey('Family', row.item.id)\"\n              >Delete Reservation</b-button>\n            </template>\n          </b-table>\n        </b-col>\n\n        <b-col cols=\"12\">\n          <h2 class>Room - Deluxe</h2>\n          <b-img :src=\"require('@/assets/img4.jpg')\" fluid alt=\"Responsive image\"></b-img>\n          <b-table :fields=\"fields\" bordered striped hover :items=\"deluxe\">\n            <template v-slot:cell(delete)=\"row\">\n              <b-button\n                size=\"md\"\n                variant=\"danger\"\n                block\n                @click=\"deletekey('Deluxe', row.item.id)\"\n              >Delete Reservation</b-button>\n            </template>\n          </b-table>\n        </b-col>\n      </b-row>\n    </b-container>\n  </div>\n</template>\n\n<script>\nimport { db } from \"@/firebase\";\nexport default {\n  data() {\n    return {\n      single: [],\n      double: [],\n      family: [],\n      deluxe: [],\n      clients: [],\n      messages: [],\n      fields: [\n        { key: \"id\", label: \"Id\", sortable: true },\n        // { key: \"clientID\", label: \"ClientID\", sortable: true },\n        { key: \"firstname\", label: \"Firstname\", sortable: true },\n        { key: \"lastname\", label: \"Lastname\", sortable: true },\n        { key: \"nationality\", label: \"Nationality\", sortable: true },\n\n        { key: \"phone\", label: \"Phone\", sortable: true },\n        { key: \"email\", label: \"E-mail\", sortable: true },\n        { key: \"room\", label: \"Room\", sortable: true },\n        { key: \"price\", label: \"Price\", sortable: true },\n        { key: \"day\", label: \"Day\", sortable: true },\n        { key: \"delete\", label: \"Delete Reservation\", sortable: true }\n      ],\n      fieldsClient: [\n        { key: \"id\", label: \"Id\", sortable: true },\n        { key: \"lastname\", label: \"lastname\", sortable: true },\n        { key: \"email\", label: \"E-mail\", sortable: true },\n        { key: \"delete\", label: \"Delete Client\", sortable: true }\n      ],\n      fieldsMessage: [\n        { key: \"id\", label: \"Id\", sortable: true },\n        { key: \"message\", label: \"Message\", sortable: true },\n        { key: \"lastname\", label: \"lastname\", sortable: true },\n\n        { key: \"email\", label: \"E-mail\", sortable: true },\n        { key: \"delete\", label: \"Delete Message\", sortable: true }\n      ]\n    };\n  },\n  created() {\n    db.collection(\"Single\").onSnapshot(querySnapshot => {\n      this.single = [];\n      querySnapshot.forEach(doc => {\n        const x = doc.data();\n        x.id = doc.id;\n        this.single.push(x);\n      });\n    });\n\n    db.collection(\"Double\").onSnapshot(querySnapshot => {\n      this.double = [];\n      querySnapshot.forEach(doc => {\n        const x = doc.data();\n        x.id = doc.id;\n        this.double.push(x);\n      });\n    });\n\n    db.collection(\"Family\").onSnapshot(querySnapshot => {\n      this.family = [];\n      querySnapshot.forEach(doc => {\n        const x = doc.data();\n        x.id = doc.id;\n        this.family.push(x);\n      });\n    });\n\n    db.collection(\"Deluxe\").onSnapshot(querySnapshot => {\n      this.deluxe = [];\n      querySnapshot.forEach(doc => {\n        const x = doc.data();\n        x.id = doc.id;\n        this.deluxe.push(x);\n      });\n    });\n\n    db.collection(\"clients\").onSnapshot(querySnapshot => {\n      this.clients = [];\n      querySnapshot.forEach(doc => {\n        const x = doc.data();\n        x.id = doc.id;\n        this.clients.push(x);\n      });\n    });\n\n    db.collection(\"messages\").onSnapshot(querySnapshot => {\n      this.messages = [];\n      querySnapshot.forEach(doc => {\n        const x = doc.data();\n        x.id = doc.id;\n        this.messages.push(x);\n      });\n    });\n  },\n\n  methods: {\n    deletekey(collection, id) {\n      db.collection(collection)\n        .doc(id)\n        .delete()\n        .then(function() {\n          console.log(\"Document successfully deleted\");\n        })\n        .catch(function(error) {\n          console.error(\"Error removing document: \", error);\n        });\n    }\n  }\n};\n</script>\n\n<style scoped>\ni {\n  color: #48a9a6;\n}\nimg {\n  height: 20xvh !important;\n  width: 100%;\n}\n</style>\n"],"sourceRoot":"src/views"}]}